            android:textSize="18sp" android:textColor="@android:color/holo_blue_dark"
            app:layout_constraintHorizontal_bias="0.101" app:layout_constraintVertical_bias="0.259"/>
    <TextView
            android:text="Laboratorio 4"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/lab4" android:textSize="18sp"
            android:textColor="@android:color/holo_blue_dark"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.101"
            app:layout_constraintVertical_bias="0.353"/>
    <WebView
            android:layout_width="368dp"
            android:layout_height="302dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintVertical_bias="0.969"
            android:id="@+id/webView"/>
</android.support.constraint.ConstraintLayout>